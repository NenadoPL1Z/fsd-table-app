  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "Rodion Pyatigorskiy",
    "email": "rodionpyatigroskiy@gmail.com",
    "url": "https://github.com/NenadoPL1Z/fsd-table-app.git"
  },
  "type": "module",
  "scripts": {
